<% if @dr_attr.errors.any? -%>
$("#body-content").html("<%= escape_javascript(render(:partial => "form"))%>");
$('#sheet_errors').show()
<% else -%>
$(":input:not(input[type=submit])").val("");
$("#dr_sheet_form").hide(100)
$("#body-content").html("<%= escape_javascript( render(:partial => "dr_sheet") ) %>");
$('#sheet_notice').show()
<% end -%>
